   "source": [
    "import numpy as np\n",
    "\n",
    "def rotLeft(a,d):\n",
    "    toMove = a[0:d]\n",
    "    for x in range(d):\n",
    "        del a[0]\n",
    "        #print(x)\n",
    "    a = a + toMove\n",
    "    return(a)"
   ]
